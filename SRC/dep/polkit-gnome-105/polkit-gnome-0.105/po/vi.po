msgstr "<small><b>Nhà cung cấp:</b></small>"

#: ../src/polkitgnomeauthenticationdialog.c:724
#, c-format
msgid "Click to open %s"
msgstr "Nhấn để mở %s"

#: ../src/polkitgnomeauthenticationdialog.c:898
msgid "Authenticate"
msgstr "Xác thực"

